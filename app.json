{
  "expo": {
    "name": "ZenTimer",
    "slug": "ZenTimer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoZenTimer2.png",
    "userInterfaceStyle": "light",
    "scheme": "zenTimerScheme",
    "splash": {
      "image": "./assets/ZenTimer1.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zentimer.app",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "This application needs access to the microphone to hear user instructions"
      }
    },
    "android": {
      "package": "com.zentimer.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoZenTimer2.png",
        "backgroundColor": "#ffffff"
      },
      "jsEngine": "jsc",
      "permissions": ["AUDIO_RECORDING"],
      "notification": {
        "sound": "./assets/alarm_2.mp3"
      }
    },
    "web": {
      "favicon": "./assets/logoZenTimer2.png"
    },
    "extra": {
      "eas": {
        "projectId": "adc59aa6-8d0c-4ae7-82dc-31d3450b45ed"
      }
    },
    "owner": "lenindelrionicaragua"
  }
}
