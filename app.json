{
  "expo": {
    "name": "ZenTimer",
    "slug": "ZenTimer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconZenTimerApp.png",
    "userInterfaceStyle": "light",
    "scheme": "zenTimer",
    "splash": {
      "image": "./assets/splashWelcomeScreen.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zentimer.app",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "This application needs access to the microphone to hear user instructions"
      }
    },
    "android": {
      "package": "com.zentimer.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconZenTimerApp.png",
        "backgroundColor": "#ffffff"
      },
      "jsEngine": "hermes",
      "permissions": ["AUDIO_RECORDING"]
    },
    "web": {
      "favicon": "./assets/iconZenTimerApp.png"
    },
    "extra": {
      "eas": {
        "projectId": "adc59aa6-8d0c-4ae7-82dc-31d3450b45ed"
      }
    },
    "owner": "lenindelrionicaragua"
  }
}
